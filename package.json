{
  "name": "@juit/pgproxy",
  "version": "1.0.0",
  "description": "PostgreSQL Proxy over HTTP and WebSockets",
  "author": "Juit Developers <developers@juit.com>",
  "license": "Apache-2.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
