    "lint": "plug lint",
    "test": "plug test",
    "transpile": "plug transpile"
  },
  "dependencies": {
    "generic-pool": "^3.9.0",
    "libpq": "^1.8.12"
  },
  "devDependencies": {
    "@plugjs/build": "^0.4.18",
    "@types/libpq": "^1.8.10",
    "typescript": "^5.1.6"
  },
  "directories": {
    "test": "test"
  },
  "files": [
    "*.md",
    "dist/",
    "src/"
  ]
